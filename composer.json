{
    "name": "ekreative/redmine_login",
    "license": "MIT",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Ekreative\\RedmineLoginBundle\\": "src/Ekreative/RedmineLoginBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/",
            "SymfonyStandard\\": "app/SymfonyStandard/"
        }
    },
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "2.7.*",
        "guzzlehttp/guzzle": "^6.0",
        "mcfedr/json-form": "^3.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2"
    },
    "require-dev": {
        "symfony/monolog-bundle": "~2.4",
        "incenteev/composer-parameter-handler": "~2.0",
        "phpunit/phpunit": "^4.7"
    },
    "suggest": {
        "nelmio/api-doc-bundle": "Provide online documentation of API functions"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "tests/parameters.yml"
        }
    }
}
