{
    "name": "ekreative/redmine_login",
    "description": "Login to symfony using redmine as user provider",
    "license": "MIT",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Ekreative\\RedmineLoginBundle\\": "src/Ekreative/RedmineLoginBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    },
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "2.7.*",
        "guzzlehttp/guzzle": "^6.0",
        "mcfedr/json-form": "^3.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "nelmio/api-doc-bundle": "^2.9"
    },
    "require-dev": {
        "symfony/monolog-bundle": "~2.4",
        "incenteev/composer-parameter-handler": "~2.0",
        "phpunit/phpunit": "^4.7"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "tests/parameters.yml",
            "env-map": {
                "redmine": "SYMFONY__REDMINE",
                "admin_user": "SYMFONY__ADMIN_USER",
                "admin_pass": "SYMFONY__ADMIN_PASS",
                "user_user": "SYMFONY__USER_USER",
                "user_pass": "SYMFONY__USER_PASS"
            }
        }
    }
}
